<Window x:Class="HageLauncher.Minecraft"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HageLauncher"
        mc:Ignorable="d"
        Title="Minecraft" Height="450" Width="800">
    <Grid Background="#FF151515">
        <StackPanel VerticalAlignment="Center">
            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="50" FontFamily="Bahnschrift" Foreground="#FF0088FF">HageClient</TextBlock>
            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="20" FontFamily="Bahnschrift" Foreground="#FF0088FF">Launching...</TextBlock>
        </StackPanel>
    </Grid>

</Window>
